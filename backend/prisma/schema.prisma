generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int     @id @default(autoincrement())
  name          String  @db.VarChar(255)
  password      String? @db.VarChar(255)
  providerId    String? @db.VarChar(255)
  isAdmin       Boolean @default(false)
}
